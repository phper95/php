	.boo-box .item .lunli {
		background-image:url('http://ser3.graphmovie.com/boo/ICON/APP/index_icon_type_ethics@3x.png');
	}
	
	
	.boo-box .item .movie {
		float:left;width:290px;margin-bottom:15px;padding:5px;position:relative;
	}
	.movie .img {
		float:left;width:100%;height:200px;
		background:url('http://avatar.graphmovie.com/boo/movies/7482/7482_20151221205214_b.jpg');
		background-size:100% 100%;position:relative;
	}
	.movie .grapher, .movie .play, .movie .ding {
		background:url('__PUBLIC__/images/apphome/hc_botbar_eye.png') no-repeat left center;
		background-size:auto 60%;float:right;padding:0 16px;
		color:#fff; text-shadow:0 0 2px #000;
	}
	.movie .grapher {
		background-image:url('__PUBLIC__/images/apphome/hc_3btn_rank.png');
		float:left;
	}
	.movie .ding {
		background-image:url('__PUBLIC__/images/apphome/hc_botbar_liked.png');
	}
	.movie .img-info {
		position:absolute;bottom:0;left:0;padding:2px 8px;width:100%;
	}
	.movie .score {
		float:right;width:36%;
	}
	.movie .ctrl {
		position:absolute;width:100%;height:100%;background-color:rgba(0,0,0,0.4);
		top:0;left:0;text-align:center;padding-top:70px;
	}
	.movie .ctrl .time {
		position:absolute;top:2px;right:2px;color:#fff;
	}
	
	
	.boo-box .item .more {text-align:center;}
	.more .refresh, .more .goto {
		background:url('http://ser3.graphmovie.com/boo/ICON/APP/change.png') no-repeat right center;
		background-size:auto 90%;padding-right:20px;font-size:1.2em;
	}
	.more .goto {
		background-image:url('http://ser3.graphmovie.com/boo/ICON/APP/more.png');
	}
	
	.boo-box .pianch {
		float:left;width:100%;margin-bottom:15px;
	}
	.pianch .img {
		float:left;width:30%;height:120px;
		background:url('http://avatar.graphmovie.com/boo/movies/7482/7482_20151221205214_b.jpg');
		background-size:100% 100%;position:relative;
	}
	.pianch .img .ctrl {
		position:absolute;width:100%;height:100%;background-color:rgba(0,0,0,0.4);
		top:0;left:0;text-align:center;padding-top:50px;
	}
	.pianch .ctrl .time {
		position:absolute;top:2px;right:2px;color:#fff;
	}
	.pianch .info {
		float:right;width:68%;position:relative; height:120px;
	}
</style>
