<include file="Public:header" />
<!-- Sidebar begins -->
<div id="sidebar">
	<include file="Public:left" />
	<!-- Secondary nav -->
</div>
<!-- Sidebar ends -->

<!-- Content begins -->
<div id="content">
    <include file="Public:bav" />
    <!-- Main content -->
    <div class="wrapper">
        	<div class="fluid">
        		<form method='post' id="form1" name="form1" action="{:U('Adv/update')}"  enctype="multipart/form-data">
                    <div class="widget ">
                        <div class="whead">
                        <h6>添加广告</h6>
                        <a href="{:getCurrentUrl()}" class="buttonH bBlue mb10 mt5">返回</a>
                        <div class="clear"></div></div>
                        <div class="formRow">
                            <div class="grid3"><label>广告名称：</label></div>
                            <div class="grid3"><input type="text" name="name" placeholder="广告名称" value="{$vo.name}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告副标题：</label></div>
                            <div class="grid7"><input type="text" name="sub_title" placeholder="广告副标题" value="{$vo.sub_title}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告编者按：</label></div>
                            <div class="grid7"><textarea rows="3" cols="" name="editor_note" placeholder="编者按">{$vo.editor_note}</textarea></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>发行公司：</label></div>
                            <div class="grid7"><input type="text" name="company" placeholder="发行公司" value="{$vo.company}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>网站地址：</label></div>
                            <div class="grid7"><input type="text" name="website" placeholder="网站地址" value="{$vo.website}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告简介：</label></div>
                            <div class="grid7"><textarea rows="4" cols="" name="intro" placeholder="广告简介">{$vo.intro}</textarea></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告评分：</label></div>
                            <div class="grid3"><input type="text" name="score" placeholder="广告评分" value="{$vo.score}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告尺寸：</label></div>
                            <div class="grid3"><input type="text" name="total_size" placeholder="广告尺寸（Mb）" value="{$vo.total_size}" /></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告类型：</label></div>
                            <div class="grid3"><input type="text" name="tags" placeholder="广告类型" value="{$vo.tags}" /></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告被标记签字串：</label></div>
                            <div class="grid9"><input type="text" name="tags_text"  placeholder="多个请用半角逗号隔开" value="{$vo.tags_text}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告关键字字串：</label></div>
                            <div class="grid9"><input type="text" name="keyword"  placeholder="多个请用半角逗号隔开" value="{$vo.keyword}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>按钮标题：</label></div>
                            <div class="grid3"><input type="text" name="open_btn_title"  placeholder="按钮标题" value="{$vo.open_btn_title}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>点击标题打开的网站：</label></div>
                            <div class="grid3"><input type="text" name="open_btn_url"  placeholder="点击标题打开的网站" value="{$vo.open_btn_url}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告作者ID：</label></div>
                            <div class="grid3"><input type="text" name="grapher"  placeholder="广告作者ID, 多个以英文半角逗号隔开" value="{$vo.grapher}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>顶次数：</label></div>
                            <div class="grid3"><input type="text" placeholder="顶次数" disabled="disabled" value="{$vo.ding}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>踩次数：</label></div>
                            <div class="grid3"><input type="text" placeholder="踩次数" disabled="disabled" value="{$vo.cai}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>播放次数：</label></div>
                            <div class="grid3"><input type="text" placeholder="播放次数" disabled="disabled" value="{$vo.played}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>分享次数：</label></div>
                            <div class="grid3"><input type="text" placeholder="分享次数" disabled="disabled" value="{$vo.share}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>收藏次数：</label></div>
                            <div class="grid3"><input type="text" placeholder="收藏次数" disabled="disabled" value="{$vo.keep}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>官网是否在线阅读：</label></div>
                            <div class="grid9">
                            	<label class="nopadding grid1" ><input type="radio" <eq name="vo.wp_post_open" value="1">checked="checked"</eq> value="1" name="wp_post_open"/>是</label>
                            	<label class="nopadding grid1"><input type="radio" <eq name="vo.wp_post_open" value="0">checked="checked"</eq> value="0" name="wp_post_open"/>否</label>
							</div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>官网文章ID：</label></div>
                            <div class="grid3"><input type="text" name="wp_post_id" placeholder="官网文章ID" value="{$vo.wp_post_id}"/></div>
                            <div class="clear"></div>
                        </div>
                        
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告是否生效：</label></div>
                            <div class="grid9">
                            	<label class="nopadding grid1" ><input type="radio" value="1" <eq name="vo.open" value="1">checked="checked"</eq> name="open"/>是</label>
                            	<label class="nopadding grid1"><input type="radio" value="0" <eq name="vo.open" value="0">checked="checked"</eq> name="open"/>否</label>
							</div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>广告使用图片服务器编号：</label></div>
                            <div class="grid9">
                            	<label class="nopadding grid1" ><input type="radio" value="0" <eq name="vo.imgserver_use" value="0">checked="checked"</eq> name="imgserver_use"/>第三方</label>
                            	<label class="nopadding" style="width:140px;"><input type="radio" value="1" <eq name="vo.imgserver_use" value="1">checked="checked"</eq> name="imgserver_use"/> 自身服务器1(230)</label>
                            	<label class="nopadding" style="width:125px;" ><input type="radio" value="2" <eq name="vo.imgserver_use" value="2">checked="checked"</eq> name="imgserver_use"/>自身服务器2(248)</label>
                            	<label class="nopadding grid3" ><input type="radio" <eq name="vo.imgserver_use" value="3">checked="checked"</eq> value="3" name="imgserver_use"/>自身服务器3(31)</label>
							</div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                        	<div class="grid3">显示支持</div>
                        	<div class="grid9">
                        		<label style="width:120px;"><input <eq name="vo.show_list" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_list" class="check" />支持-列表</label>
	                            <label style="width:120px;"><input <eq name="vo.show_window" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_window" class="check" />支持-窗口</label>
	                            <label style="width:120px;"><input <eq name="vo.show_search" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_search" class="check" />支持-搜索</label>
	                            <label style="width:120px;"><input <eq name="vo.show_tag" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_tag" class="check" />支持-Tag</label>
	                            <label style="width:120px;"><input <eq name="vo.show_qrcode" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_qrcode" class="check" />支持-二维码</label>
	                            <label style="width:120px;"><input <eq name="vo.show_same" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_same" class="check" />支持-相关推荐</label>
	                            <label style="width:120px;"><input <eq name="vo.show_grapher" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_grapher" class="check" />支持-作品列表</label>
	                            <label style="width:120px;"><input <eq name="vo.show_read" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_read" class="check" />支持-阅读时</label>
	                            <label style="width:120px;"><input <eq name="vo.show_fullscreen" value="1">checked="checked"</eq> type="checkbox" value="1" name="show_fullscreen" class="check" />支持-全屏</label>
                        	</div>
                        	<div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                        	<div class="grid3">相对操作</div>
                        	<div class="grid9">
                        		<label style="width:200px;"><input type="checkbox" value="1" <eq name="vo.show_detail_fullscreen" value="1">checked="checked"</eq> name="show_detail_fullscreen" class="check" />全屏广告点击时进入详情</label>
	                            <label style="width:200px;"><input type="checkbox" value="1" <eq name="vo.show_detail_window" value="1">checked="checked"</eq> name="show_detail_window" class="check" />窗口广告点击时进入详情</label>
	                            <label style="width:200px;"><input type="checkbox" value="1" <eq name="vo.show_detail_list" value="1">checked="checked"</eq> name="show_detail_list" class="check" />列表广告点击时进入详情</label>
	                            <label style="width:200px;"><input type="checkbox" value="1" <eq name="vo.show_detail_read" value="1">checked="checked"</eq> name="show_detail_read" class="check" />阅读广告点击时进入详情</label>
	                            <label style="width:200px;"><input type="checkbox" value="1" <eq name="vo.show_detail_same" value="1">checked="checked"</eq> name="show_detail_same" class="check" />相关推荐点击时进入详情</label>
                        	</div>
                        	<div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                            <div class="grid3"><label>全屏广告展示时间：</label></div>
                            <div class="grid3"><input type="text" name="fullscrenn_show_second" value="{$vo.fullscrenn_show_second}" placeholder="秒数" /></div>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="formRow">
                        	<input type="hidden" value="{$vo.id}" name="id">
                        	<input type="submit" value="提 交" class="buttonM bBlack formSubmit" /><div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>
                    </form>
            </div>
    </div>
    <!-- Main content ends -->
    
</div>
<!-- Content ends -->

<include file="Public:footer" />
