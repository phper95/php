<include file="Public:header" />
<!-- Sidebar begins -->
<div id="sidebar">
	<include file="Public:left" />
	<!-- Secondary nav -->
    <div class="secNav">
        <div class="secWrapper">
            <div class="secTop">
                <div class="balance">
                    <div class="balInfo">上次登陆:<span>{$Think.session.lastLoginTime|toDate}</span></div>
                </div>
            </div>


            <div class="divider"><span></span></div>
            
       </div> 
       <div class="clear"></div>
   </div>
</div>
<!-- Sidebar ends -->
    
<!-- Content begins -->
<div id="content">
    <div class="contentTop">
        <span class="pageTitle">主页君</span>
        <div class="clear"></div>
    </div>
    
    <!-- Breadcrumbs line -->
    <div class="breadLine">
        <div class="bc">
            <ul id="breadcrumbs" class="breadcrumbs">
                <li><a href="#">磁盘管理</a></li>
                
            </ul>
        </div>
        
        
    </div>
    
    <!-- Main content -->
    <div class="wrapper">
    	<div class="widget">
    		<div class="whead">
    			<h6>磁盘清理</h6>
    			<a href="{:U('Index/index')}" class="buttonH bBlue mb10 mt5">返回</a>
    			<div class="clear"></div>
    		</div>
	    	<div class="body">
	    		本次清理可清理出  <span class="greenBack" onclick="">{:getFomartFileSize($t_size)}</span> 空间。 <a href="{:U('Index/diskManage',array('do'=>true))}" class="blueBack"> 确定清理  </a>
	    		<div>
	    			<div class="widget">
	    			<div class="whead"><h6>可清理日志</h6><div class="clear"></div></div>
	    			<div class="body">
	    			<volist name="t_info.ok" id="vo">
	    				<div>{$vo}</div>
	    			</volist>
	    			</div>
	    			</div>
	    			
	    			<!--<div class="divider"><span></span></div>
	    			--><div class="widget">
	    			<div class="whead"><h6>不可清理日志</h6><div class="clear"></div></div>
	    			<div class="body">
	    			<volist name="t_info.no" id="vo">
	    				<div class="red">{$vo}</div>
	    			</volist>
	    			</div>
	    			</div>
	    		</div>
	    	</div>
    	</div>
    </div>
    <!-- Main content ends -->
    
</div>
<!-- Content ends -->

<include file="Public:footer" />
